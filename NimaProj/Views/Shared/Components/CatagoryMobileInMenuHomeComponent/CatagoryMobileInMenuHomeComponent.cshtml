@model IEnumerable<TblCatagory>

<ul class="sub-menu">
    @foreach (var item in Model)
    {
        if (item.InverseParent.Any())
        {
            <li class="mega-menu-col">
                <a class="title-megamenu-mobile ">@item.Name </a>
                <ul>
                    @foreach (var j in item.InverseParent)
                    {
                        <li>
                            <a href="/Product/@item.CatagoryId/@item.Name.Replace(" ","-")">
                                @j.Name
                            </a>
                        </li>
                    }
                </ul>
            </li>
        }
        else
        {
            <li>
                <a class="title-megamenu-mobile " href="/Product/@item.CatagoryId/@item.Name.Replace(" ","-")">@item.Name </a>
            </li>
        }
    }
</ul>